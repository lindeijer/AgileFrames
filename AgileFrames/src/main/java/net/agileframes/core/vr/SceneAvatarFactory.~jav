package net.agileframes.core.vr;

import net.agileframes.vr.space3d.SceneAvatar3D;

public class SceneAvatarFactory extends AvatarFactory {
  public SceneAvatar3D avatar = null;

  public SceneAvatarFactory(){}

  public void createAvatar(Body body) {
    System.out.println("SceneFactory creates Avatar");
    avatar = new SceneAvatar3D(body);
  }
  public Avatar getAvatar() { return avatar; }
  public Object getAvatarBG() { return avatar.getBG(); }

}